<!-- Views/News/Index.cshtml -->
@model IEnumerable<NewsArticle>

<h2>News Articles</h2>

<ul>
    @foreach (var article in Model)
    {
        <li>
            @article.Title
            [<a asp-action="Edit" asp-route-id="@article.Id">Edit</a>]
            [<a asp-action="Delete" asp-route-id="@article.Id">Delete</a>]
        </li>
    }
</ul>

<a asp-action="Create">Create New Article</a>

